{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://our.job.com/jobNew.schema.json",
    "type": "object",
    "properties": {
      "title": {
        "type": "string",
        "minLength": 1,
        "maxLength": 255
      },
      "salary": {
        "type": "integer",
        "minimum": 0 
      },
      "equity": {
        "type": "string",
        "minLength": 0,
        "maxLength": 255
      },
      "company_handle": {
        "type": "string",
        "minLength": 0,
        "maxLength": 255
      }
    },
    "additionalProperties": false,
    "required": [
      "title",
      "salary",
      "equity",
      "company_handle"
    ]
  }
  